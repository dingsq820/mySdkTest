  "scripts": {
    "copy:config": "mv .yarnrc.yml yarnrc.yml  && cp ../config/client-cert.pem client-cert.pem && cp ../config/client-key.pem client-key.pem && cp ../config/.yarnrc.yml .yarnrc.yml",
    "reset:config": "rm -rf client-cert.pem && rm -rf client-key.pem && rm -rf .yarnrc.yml && mv yarnrc.yml .yarnrc.yml",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint --color ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{ts,tsx}",
    "prettier": "prettier --check ./src/**/*.{ts,tsx}",
    "prettier:fix": "prettier --write ./src/**/*.{ts,tsx}",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:webpack": "rm -rf dist && tsc --project tsconfig.build.json && webpack"
  },
